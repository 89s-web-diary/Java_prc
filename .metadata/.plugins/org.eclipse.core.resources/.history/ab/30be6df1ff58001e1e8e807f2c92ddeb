package 제어문.조건문;

import java.util.Scanner;

public class Switch확인문제 {

	public static void main(String[] args) {
		
		System.out.print("당신이 먹고 싶은 점심 메뉴는 >>> ");
		Scanner sc = new Scanner(System.in);
		String name = sc.next();
		
		switch (name) {
		case "짜장면": System.out.println("중국집으로 가요"); break;
		case "라면":System.out.println("분식집으로 가요"); break;
		case "회":System.out.println("횟집으로 가요"); break;
		default: System.out.println("그냥 안먹어요"); break;
		}
		
	}

}

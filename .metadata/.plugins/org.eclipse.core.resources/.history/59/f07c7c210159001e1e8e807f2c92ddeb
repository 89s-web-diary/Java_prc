package 제어문.조건문;

import java.util.Date;

public class DateTest {

	public static void main(String[] args) {
		
		Date date = new Date();
		int hour = date.getHours();
		int min = date.getMinutes();
		int sec = date.getSeconds();
		System.out.println("현재 시간은 "+hour+"시 "+min+"분 "+sec+"초입니다");
		
	}

}
